require("assertions")
require("dspec")

describe("Array") ->
  it("can be accessed") ->
    array: [1, 2, 3]
    assert-equal(array[0], 1)

  it("can add arrays together") ->
    assert-equal([1, 2, 3] + [4], [1, 2, 3, 4])

  it("can subtract arrays from each other") ->
    assert-equal([1, 1, 2, 2, 3, 3, 4, 5] - [1, 2, 4], [3, 3, 5])
