describe(Object) ->
  it("holds instance variables") ->
    Wombat:
      init:
        @wombat: "Wombat"
      value:
        @wombat
    wombat: Wombat()
    assert-equal(wombat.value, "Wombat")

  it("starts instance variables off as void") ->
    Wallaby:
      value:
        @wallaby
    wallaby: Wallaby()
    assert-equal(wallaby.value, void)
